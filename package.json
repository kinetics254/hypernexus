{
  "name": "@brainspore/hypernexus",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node --loader ts-node/esm test.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "test:unit": "vitest run",
    "test:e2e": "vitest run --env=e2e",
    "test": "tsx tests/unit/test.mts",
    "generate-docs": "typedoc --out docs src",
    "lint": "eslint src --ext js,mjs,cjs,ts",
    "format": "prettier --write 'src/**/*.{js,mjs,cjs,ts}'"
  },
  "keywords": [],
  "author": "Paullaster Okoth",
  "license": "MIT",
  "description": "A lightWeight, robust and easily extensible HTTP(/2,/3)/HTTPS network transport utility",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.0",
    "globals": "^15.14.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "axios-ntlm": "^1.4.3",
    "bottleneck": "^2.19.5",
    "dotenv": "^16.4.7",
    "node-cache": "^5.1.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  }
